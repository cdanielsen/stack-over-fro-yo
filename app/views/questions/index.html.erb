<div class="row">
  <h1 class="text-center">THE GODDAMN LIST!</h1>
</div>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <ul class="unbullet">
      <% @questions.each_with_index do |question, i| %>
        <li><%= link_to i+1, question_path(question) %>:  <%= question.content %></li>
        <% end %>
    </ul>

  </div>
</div>
